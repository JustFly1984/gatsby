{
  "name": "gatsby-source-drupal",
  "description": "Gatsby source plugin for building websites using the Drupal CMS as a data source",
  "version": "6.14.0-next.2",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-drupal#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@opentelemetry/semantic-conventions": "^1.23.0",
    "agentkeepalive": "^4.5.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "fastq": "^1.17.1",
    "gatsby-plugin-utils": "workspace:*",
    "gatsby-source-filesystem": "workspace:*",
    "got": "^14.2.1",
    "http2-wrapper": "^2.2.1",
    "lodash": "^4.17.21",
    "opentracing": "^0.14.7",
    "probe-image-size": "^7.2.3",
    "tiny-async-pool": "^2.1.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/lodash": "4.17.0",
    "@types/bluebird": "3.5.42",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "babel-preset-gatsby-package": "workspace:*",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "workspace:*",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-source-drupal"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production pnpm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "test": "npx jest ./src/__tests__ --runInBand",
    "test:watch": "npx jest --watch ./src/__tests__ --runInBand"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
