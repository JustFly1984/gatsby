{
  "packageManager": "pnpm@9.0.5",
  "name": "gatsby-plugin-flow",
  "version": "4.14.0-next.2",
  "description": "Provides drop-in support for Flow by adding @babel/preset-flow.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production pnpm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "andrew@gatsbyjs.com",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-flow#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-flow"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/preset-flow": "^7.24.1",
    "@babel/runtime": "^7.24.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "babel-preset-gatsby-package": "^3.14.0-next.2",
    "cross-env": "^7.0.3",
    "gatsby-plugin-utils": "^4.14.0-next.2"
  },
  "peerDependencies": {
    "gatsby": "^5.14.0-next.2",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
