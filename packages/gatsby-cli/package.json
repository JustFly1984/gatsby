{
  "packageManager": "pnpm@9.0.5",
  "name": "gatsby-cli",
  "description": "Gatsby command-line interface for creating new sites and running Gatsby commands",
  "version": "5.14.0-next.2",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bin": {
    "gatsby": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/code-frame": "^7.24.2",
    "@babel/core": "^7.24.4",
    "@babel/generator": "^7.24.4",
    "@babel/helper-plugin-utils": "^7.24.0",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.4",
    "@babel/template": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@types/common-tags": "^1.8.4",
    "better-opn": "^3.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "common-tags": "^1.8.2",
    "convert-hrtime": "^5.0.0",
    "create-gatsby": "workspace:*",
    "envinfo": "^7.12.0",
    "execa": "^8.0.1",
    "fs-exists-cached": "^1.0.0",
    "fs-extra": "^11.2.0",
    "gatsby-core-utils": "workspace:*",
    "gatsby-telemetry": "workspace:*",
    "hosted-git-info": "^7.0.1",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "is-valid-path": "^0.1.1",
    "joi": "^17.13.0",
    "lodash": "^4.17.21",
    "opentracing": "^0.14.7",
    "pretty-error": "^4.0.0",
    "progress": "^2.0.3",
    "prompts": "^2.4.2",
    "redux": "5.0.1",
    "resolve-cwd": "^3.0.0",
    "semver": "^7.6.0",
    "signal-exit": "^4.1.0",
    "@types/stack-trace": "0.0.33",
    "stack-trace": "^1.0.0-pre2",
    "strip-ansi": "^7.1.0",
    "yargs": "^17.7.2",
    "yoga-layout-prebuilt": "^1.10.0",
    "yurnalist": "^2.1.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__helper-plugin-utils": "^7.10.3",
    "@types/babel__core": "7.20.5",
    "@types/semver": "7.5.8",
    "@types/react": "^18.3.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@babel/cli": "^7.24.1",
    "@types/lodash": "4.17.0",
    "@sigmacomputing/babel-plugin-lodash": "^3.3.5",
    "@types/hosted-git-info": "^3.0.5",
    "@types/yargs": "^17.0.32",
    "@types/signal-exit": "^3.0.4",
    "babel-preset-gatsby-package": "workspace:*",
    "cross-env": "^7.0.3",
    "npm-run-all": "4.1.5",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.16.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-internal": "^1.0.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "files": [
    "lib/",
    "scripts/",
    "cli.js"
  ],
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-cli#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-cli"
  },
  "scripts": {
    "build:babel": "babel src --out-dir lib --ignore \"**/__tests__\" --ignore \"src/reporter/loggers/ink/**/*\" --extensions \".ts,.js,.tsx\"",
    "build:rollup": "rollup -c",
    "build": "npm-run-all --npm-path pnpm -p build:babel build:rollup",
    "prepare": "cross-env NODE_ENV=production pnpm run build && pnpm run typegen",
    "typegen": "rimraf --glob \"lib/**/**.d.ts\" && tsc --emitDeclarationOnly --declaration --declarationDir lib/",
    "watch:babel": "pnpm run build:babel -- --watch",
    "watch:rollup": "pnpm run build:rollup -- -w",
    "watch": "npm-run-all --npm-path pnpm -p watch:babel watch:rollup",
    "postinstall": "node scripts/postinstall.mjs"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
