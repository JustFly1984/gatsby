{
  "packageManager": "pnpm@9.0.5",
  "name": "gatsby-plugin-coffeescript",
  "description": "Adds CoffeeScript support for Gatsby",
  "version": "5.14.0-next.2",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "contributors": [
    "Noah Lange <noahrlange@gmail.com>"
  ],
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-coffeescript#readme",
  "keywords": [
    "coffeescript",
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "coffee-loader": "^5.0.0",
    "coffee-react-transform": "^5.0.0",
    "coffeescript": "^2.7.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "babel-preset-gatsby-package": "workspace:*",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "workspace:*",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-coffeescript"
