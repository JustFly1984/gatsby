{
  "name": "gatsby-plugin-mdx",
  "version": "5.14.0-next.2",
  "description": "MDX integration for Gatsby",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-mdx#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-mdx"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "acorn": "^8.11.3",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.8.6",
    "deepmerge": "^4.3.1",
    "estree-util-build-jsx": "^3.0.1",
    "fs-extra": "^11.2.0",
    "gatsby-core-utils": "workspace:*",
    "gatsby-plugin-utils": "workspace:*",
    "gray-matter": "^4.0.3",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-to-hast": "^13.1.0",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-toc": "^7.0.0",
    "rehype-infer-description-meta": "^2.0.0",
    "remark-unwrap-images": "^4.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@jest/types": "29.6.3",
    "@types/jest": "29.5.12",
    "jest": "29.7.0",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@types/estree": "^1.0.5",
    "@types/mdast": "^4.0.3",
    "@types/unist": "^3.0.2",
    "babel-preset-gatsby-package": "workspace:*",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "opentracing": "^0.14.7",
    "typescript": "5.4.5",
    "webpack": "^5.91.0"
  },
  "peerDependencies": {
    "@mdx-js/react": "^3.0.1",
    "gatsby": "workspace:*",
    "gatsby-source-filesystem": "workspace:*",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "mdx",
    "markdown",
    "remark",
    "rehype"
  ],
  "files": [
    "dist/*",
    "gatsby-node.js"
  ],
  "scripts": {
    "build": "babel src --out-dir dist/ --ignore \"**/__tests__\" --ignore \"**/__mocks__\" --extensions \".ts\"",
    "typegen": "tsc --emitDeclarationOnly --declaration --declarationDir dist/",
    "prepare": "cross-env NODE_ENV=production pnpm run clean && pnpm run build && pnpm run typegen",
    "watch": "babel -w src --out-dir dist/ --ignore \"**/__tests__\" --extensions \".ts,.tsx\"",
    "test": "jest",
    "clean": "del-cli dist/*"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
