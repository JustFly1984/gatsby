{
  "name": "gatsby-plugin-page-creator",
  "version": "5.14.0-next.2",
  "description": "Gatsby plugin that automatically creates pages from React components in specified directories",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production pnpm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "Steven Natera <tektekpush@gmail.com> (https://twitter.com/stevennatera)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-page-creator"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-page-creator#readme",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@babel/traverse": "^7.24.1",
    "@sindresorhus/slugify": "^2.1.1",
    "chokidar": "^3.6.0",
    "fs-exists-cached": "^1.0.0",
    "fs-extra": "^11.2.0",
    "gatsby-core-utils": "workspace:*",
    "gatsby-page-utils": "workspace:*",
    "gatsby-plugin-utils": "workspace:*",
    "gatsby-telemetry": "workspace:*",
    "globby": "^14.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/lodash": "4.17.0",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "babel-preset-gatsby-package": "workspace:*",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "workspace:*",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
