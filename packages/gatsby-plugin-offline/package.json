{
  "name": "gatsby-plugin-offline",
  "description": "Gatsby plugin which sets up a site to be able to run offline",
  "version": "6.14.0-next.2",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-offline#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "offline",
    "precache",
    "service-worker"
  ],
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "cheerio": "^1.0.0-rc.12",
    "gatsby-core-utils": "workspace:*",
    "glob": "^10.3.12",
    "idb-keyval": "^6.2.1",
    "lodash": "^4.17.21",
    "workbox-build": "^7.1.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@types/lodash": "4.17.0",
    "babel-preset-gatsby-package": "workspace:*",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "gatsby-plugin-utils": "workspace:*",
    "rewire": "^7.0.0"
  },
  "peerDependencies": {
    "gatsby": "workspace:*",
    "react": "^18.2.0 || ^0.0.0",
    "react-dom": "^18.2.0 || ^0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-offline"
  },
  "scripts": {
    "build": "pnpm run build:src && pnpm run build:sw-append",
    "build:src": "babel src --out-dir . --ignore \"**/__tests__,src/sw-append.js\"",
    "build:sw-append": "cpy \"sw-append.js\" \"../\" --cwd=./src",
    "prepare": "cross-env NODE_ENV=production pnpm run build",
    "watch": "pnpm run build:sw-append -- --watch & pnpm run build:src -- --watch"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
