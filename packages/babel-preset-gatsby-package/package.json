{
  "packageManager": "pnpm@9.0.6",
  "name": "babel-preset-gatsby-package",
  "version": "3.14.0-next.2",
  "author": "Philipp Spiess <hello@philippspiess.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/babel-preset-gatsby-package"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/babel-preset-gatsby-package#readme",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/plugin-transform-typescript": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/runtime": "^7.24.4",
    "@sigmacomputing/babel-plugin-lodash": "^3.3.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "core-js": "^3.37.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/helper-plugin-test-runner": "7.24.1",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@types/babel__core": "^7.20.5",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.24.4"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=20.12.2"
  },
  "files": [
    "lib/*.js"
  ],
  "scripts": {
    "build": "babel src --out-dir lib/ --ignore \"**/__tests__\" --extensions \".js\"",
