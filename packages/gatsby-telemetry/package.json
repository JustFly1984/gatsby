{
  "name": "gatsby-telemetry",
  "description": "Gatsby Telemetry",
  "version": "4.14.0-next.2",
  "author": "Jarmo Isotalo <jarmo@isotalo.fi>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/code-frame": "^7.24.2",
    "@babel/runtime": "^7.24.4",
    "@turist/fetch": "^7.2.0",
    "@turist/time": "^0.0.2",
    "boxen": "^7.1.1",
    "configstore": "^6.0.0",
    "fs-extra": "^11.2.0",
    "gatsby-core-utils": "^4.14.0-next.2",
    "git-up": "^7.0.0",
    "is-docker": "^3.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "babel-preset-gatsby-package": "^3.14.0-next.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.4"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-telemetry#readme",
  "keywords": [
    "telemetry"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-telemetry"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore \"**/__tests__\",\"**/__mocks__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production pnpm run build && pnpm run typegen",
    "postinstall": "node src/postinstall.js || true",
    "typegen": "rimraf --glob \"lib/**/*.d.ts\" && tsc --emitDeclarationOnly --declaration --declarationDir lib/",
    "watch": "babel -w src --out-dir lib --ignore \"**/__tests__\",\"**/__mocks__\" --extensions \".ts,.js\""
  },
  "yargs": {
    "boolean-negation": false
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
