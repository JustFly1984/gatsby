    "@babel/traverse": "^7.24.1",
    "bluebird": "^3.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/bluebird": "3.5.42",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
