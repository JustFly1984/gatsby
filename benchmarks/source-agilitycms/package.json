{
  "name": "agility-gatsby-starter",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "2.3.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "pnpm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "setup": "NODE_ENV=development node bin/setup.js",
    "data-update": "NODE_ENV=development node bin/update.js"
  },
  "dependencies": {
    "@agility/content-fetch": "2.0.0",
    "@agility/content-management": "0.5.0",
    "@agility/gatsby-source-agilitycms": "3.0.1",
    "async": "3.2.5",
    "dotenv": "16.4.5",
    "gatsby": "5.13.4",
    "gatsby-image": "3.13.1",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-react-helmet": "6.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "truncate-html": "1.1.1"
  },
  "devDependencies": {
    "prettier": "3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agility/agility-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
