{
  "name": "kontent-gatsby-benchmark",
  "description": "Example site for Kontent as a source for \"Will It Build\"",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:send": "cross-env BENCHMARK_REPORTING_URL=true gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "pnpm run develop",
    "serve": "gatsby serve",
    "data-update": "cross-env NODE_ENV=production node update-article.js"
  },
  "dependencies": {
    "@kentico/gatsby-source-kontent": "8.1.0",
    "@rshackleton/gatsby-transformer-kontent-image": "2.1.0",
    "dotenv": "16.4.5",
    "gatsby": "5.13.4",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-benchmark-reporting": "3.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@kentico/kontent-delivery": "11.13.0",
    "@kentico/kontent-management": "2.9.2",
    "cross-env": "7.0.3",
    "prettier": "3.2.5",
    "rxjs": "7.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
