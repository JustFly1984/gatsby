{
  "name": "gatsby-benchmark-contentful",
  "private": true,
  "description": "Benchmark site for sourcing from Contentful",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "data-update": "cross-env NODE_ENV=production node bin/site.js data-update",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "site": "cross-env NODE_ENV=development node bin/site.js",
