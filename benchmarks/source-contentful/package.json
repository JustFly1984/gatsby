    "gatsby": "5.13.4",
    "gatsby-image": "3.11.1",
    "gatsby-plugin-benchmark-reporting": "3.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-source-contentful": "8.13.1",
    "gatsby-source-drupal": "6.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "yargs": "17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby/benchmarks/source-contentful"
