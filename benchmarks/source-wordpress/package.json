{
  "name": "gatsby-benchmark-wordpress",
  "private": true,
  "description": "A WordPress Benchmark site",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:send": "cross-env BENCHMARK_REPORTING_URL=true gatsby build",
